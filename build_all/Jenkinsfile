pipeline{
    agent any
    stages {
        stage ('Cleanup') {
            steps {
                deleteDir()
            }            
        }
        
        stage ('Start Test1 Pipeline') {
            steps {
                build job: 'Test1', propagate: true, wait: true
            }
        }
        stage ('Start Test2 Pipeline') {
            steps {
                build job: 'Test2', propagate: true, wait: true
            }
        }    
      
        stage ('Close Out') {
            steps {
                echo "See individual tests for Cucumber Reports"
                echo "CUCUMBER tests report: ${BUILD_URL}"
                echo "CUCUMBER tests report: ${BUILD_URL}cucumber-html-reports_53e47127-7e8c-38f4-bc2a-429191699127/overview-features.html"
            
            }    
        }    
    }          
  
}
