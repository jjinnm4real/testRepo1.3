pipeline{
    agent any
    stages {
        stage ('Cleanup') {
            steps {
                deleteDir()
            }            
        }
        stage ('Clone Repo'){
            steps {
                sh "git clone https://github.com/jjinnm4real/testRepo_v1.3.git"
                    
            }
        }
        
        stage ('Build') {
            steps {
                sh "pwd"
                 
            }        
        }
        
    }
    post {
        always {
            cucumber 'testRepo_v1.3/MICA L0 Test2/cucumber.json'
            
        }    
    }     
    
}   
