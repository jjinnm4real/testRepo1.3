pipeline{
    agent any
    stages {
        stage ('Cleanup') {
            steps {
                deleteDir()
            }            
        }
        stage ('Clone GitHub Repo'){
            steps {
                sh "git clone https://github.com/jjinnm4real/testRepo_v1.3.git"
                    
            }
        }
        stage ('Clone Bitbucket Repo'){
            steps {
                sh "git clone https://jedmister@bitbucket.org/jedmister/v1.3_coderepo.git"
            }
        }    
        
        stage ('Confirm Clone: GitHub Local Repo Files') {
            steps {
                dir("testRepo_v1.3/MICA L0 Test1/") {
                    bat "dir.bat"
                }    
                
            }
        }
        stage ('Confirm Cone: Bitbucket Local Repo Files') {
            steps {
                dir("v1.3_coderepo") {
                    echo "Bitbucket v1.3 - MICA L0 Test1"
                }  
            }
        }
        stage ('Run Test') {
            steps {
                dir("testRepo_v1.3/MICA L0 Test1/") {
                    bat "dir.bat"
                }    
            }
        }
    }   
